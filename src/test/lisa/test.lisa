(define (fact n) 
    (define (go n x) 
        (println! "Computing:" n) 
        (if (= n 0) x (go (- n 1) (* n x)))) 
    (define (id x) x)
    (id (go n 1)))

(define result (fact 5))

(println! "The result is " result)

