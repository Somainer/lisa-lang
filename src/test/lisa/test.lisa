(define sum 
    (lambda (n) (if (= n 0) 0 (+ (sum (- n 1)) n))))
(define fib (lambda (n) (if (= n 0) 0 (if (= n 1) 1 (+ (fib (- n 1)) (fib (- n 2)))))))

